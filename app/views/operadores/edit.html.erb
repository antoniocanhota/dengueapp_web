<div class="row-fluid">
  <%= render "menu" %>
  <div class="span10">
    <%- model_class = @operador.class -%>
    <h4>Editar Dados do Operador #<%= @operador.id %></h4>
    <hr>
        <%= form_for @operador ,:url => operador_path(@operador), :method => :put ,:html => { :class => 'form-horizontal' } do |f| %>
            <%= render 'form', :f => f %>
        <% end %>
  </div>
</div>
